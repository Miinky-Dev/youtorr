DROP TABLE IF EXISTS "videos";
CREATE TABLE "videos" ("id" INTEGER PRIMARY KEY  NOT NULL , "url" TEXT, "name" TEXT, "desc" TEXT, "provider" TEXT, "torrent_file" TEXT,"magnetlink" TEXT, "add_date" INTEGER, "id_channel" INTEGER);
